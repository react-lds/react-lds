{
  "name": "react-lds",
  "private": true,
  "version": "0.2.0",
  "description": "Salesforce Lightning Design System React Components",
  "keywords": [
    "react",
    "react-component",
    "salesforce lightning design system",
    "lightning design system",
    "slds",
    "react-lds"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./docs/server.js",
    "test": "jest",
    "test-watch": "jest --watch=all",
    "build": "npm run build:babel && npm run build:copy-files",
    "build:babel": "babel ./src --ignore *.spec.js --out-dir ./dist",
    "build:copy-files": "cp ./src/styles.css ./dist/",
    "prebuild": "rm -rf ./dist",
    "lint": "eslint src docs"
  },
